module example;

export package Circle {
    internal static f64 pi = 3.1416;
    internal i64 r = 25;

    export f64 getArea() {
        return pi * f64(r * r);
    }
    export f64 getCircumference () {
        return 2.0 * pi * f64(r);
    }
    export i64 getRadius() {
        return r;
    }
    export setRadius(i64 newRadius) {
        r = newRadius;
    }
}
